                <class name="organizations_with_extended_version.AddingOrganizationsWithExtendedVersion"></class>
                <class name="organizations_with_extended_version.ListOfEmployees"></class>
                <class name="catalog.MenuInTheCatalog"></class>
                <class name="catalog.Catalog"></class>
                <class name="basket.Basket"></class>
                <class name="making_orders.MakingOrders"></class>
                <class name="my_orders_history.MyOrdersHistory"></class>
                <class name="documents.Documents"></class>
                <class name="events.Events"></class>
                <class name="mean_page.MeanPageCalendar"></class>
                <class name="mean_page.MeanPageDesktop"></class>
                <class name="mean_page.MeanPageSettingPage"></class>
                <class name="technical_support.TechnicalSupport"></class>
                <class name="order_templates.OrderTemplates"></class>
                <class name="dangerous_tests.multi_regions.MultiRegions"></class>
                <class name="dangerous_tests.DangerousTests"></class>
            </classes>
        </test>


