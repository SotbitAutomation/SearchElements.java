<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression">
        <test name="Regression1 with black type of hat, catalog in filter and extended version">
            <classes>
                <class name="SettingUpCabinetForTesting.SettingUpCabinetForTesting">
                    <methods>
                        <include name="configureTheBlackHeaderInTheCabinetAndTheCatalogSectionsInTheFilter"/>
                        <include name="enableExtendedModeAndConfirmRegistrations"/>
                    </methods>
                </class>
                <class name="BeforeTest.SettingUpAutotestsForB2BSettings"></class>
                <class name="RegistrationAndAuthorization.AuthorizationB2B"></class>
                <class name="RegistrationAndAuthorization.RegistrationB2B"></class>
                <class name="OrganizationsWithExtendedVersion.AddingOrganizationsWithExtendedVersion"></class>
                <class name="OrganizationsWithExtendedVersion.ListOfEmployees"></class>
                <class name="Catalog.Catalog"></class>
                <class name="Basket.Basket"></class>
                <class name="MakingOrders.MakingOrders"></class>
                <class name="MyOrdersHistory.MyOrdersHistory"></class>
                <class name="Documents.Documents"></class>
                <class name="Events.Events"></class>
                <class name="MeanPage.MeanPageCalendar"></class>
                <class name="MeanPage.MeanPageDesktop"></class>
                <class name="MeanPage.MeanPageSettingPage"></class>
                <class name="TechnicalSupport.TechnicalSupport"></class>
                <class name="DangerousTests.DangerousTests"></class>
            </classes>
        </test>


